<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yazan.dahood.yazantasksmanger">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/main_photo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SingUpActivity" />
        <activity
            android:name=".TasksListsActivity"
            android:label="@string/title_activity_tasks_lists"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".AddTaskActivity"></activity>
    </application>

</manifest>